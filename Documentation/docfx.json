{
    "metadata": [
      {
        "src": [
          {
            "files": [
              "**.csproj"
            ],
            "exclude": [ "**/Corvus.Identity.Examples*", "**/*Specs/" ],
            "src": "../Solutions"
          }
        ],
        "dest": "api",
        "properties": {
          "TargetFramework": "netcoreapp3.1"
        },
        "disableGitFeatures": false,
        "disableDefaultFilter": false
      }
    ],
    "build": {
      "content": [
        {
          "files": [
            "api/**.yml",
            "api/**.md"
          ]
        },
        {
          "files": [
            "articles/**.md",
            "articles/**/toc.yml",
            "toc.yml",
            "*.md"
          ]
        }
      ],
      "resource": [
        {
          "files": [
            "images/**"
          ]
        }
      ],
      "overwrite": [
        {
          "files": [
            "apidoc/**.md",
            "namespaces/**.md"
          ],
          "exclude": [
            "obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "../.docs",
      "globalMetadataFiles": [],
      "fileMetadataFiles": [],
      "template": [
        "default"
      ],
      "postProcessors": [],
      "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
      "markdownEngineName": "markdig",
      "noLangKeyword": false,
      "keepFileLink": false,
      "cleanupCacheHistory": false,
      "disableGitFeatures": false
    }
  }